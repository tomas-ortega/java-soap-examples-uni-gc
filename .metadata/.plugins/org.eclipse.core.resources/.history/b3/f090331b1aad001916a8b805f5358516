package com.java.tutorial;

public class Person {
	private Integer id;
	protected String name;
	public String dni;
	
	public Person() {
		this.id = null;
	}
	
	public Person(Integer id, String name, String dni) {
		this.id = id;
		this.name = name;
		this.dni = dni;
	}
	
	public void setId(Integer id) {
		this.id = id;
	}
	
	protected class Driver {
		
	}
	
}


