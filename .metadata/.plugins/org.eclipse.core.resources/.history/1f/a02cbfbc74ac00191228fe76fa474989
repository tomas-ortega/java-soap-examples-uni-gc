package com.java.tutorial;

public class CabifyDriver extends Person 
						implements IPerson{

	@Override
	public String sayHello() {
		return "Buenos d√≠as, le cojo su maleta!";
	}
	
	public void main(String[] args) {
		CabifyDriver driver = new CabifyDriver();
		
