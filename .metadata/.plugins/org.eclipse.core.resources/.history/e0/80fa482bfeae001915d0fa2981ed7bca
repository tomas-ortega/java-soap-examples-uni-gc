           xmlns:unigest="http://www.miexample.com/studentNameType"
			targetNamespace="http://www.w3.org/2001/XMLSchema">
  <xs:simpleType name="studentNameType">
        <xs:restriction base="xs:string">
            <xs:pattern value="Pako|Juanito"/>
        </xs:restriction>
    </xs:simpleType>
  
<xs:element name="StudentDTO">
    <xs:complexType>
        <xs:sequence>
            <xs:element name="id" type="xs:string" minOccurs='1' maxOccurs='1' />
            <xs:element name="name" type="unigest:studentNameType" minOccurs='1' maxOccurs='1'/>
        </xs:sequence>
    </xs:complexType>
</xs:element>
</xs:schema>






