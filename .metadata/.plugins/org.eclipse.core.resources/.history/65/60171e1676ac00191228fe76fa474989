package com.java.tutorial;

public class App 
{
    public static void main(String[] args)
    {
       int hello;
       Integer myFirstNumber;
       
       hello = 3;
       myFirstNumber = 4;
       
       if (hello == 3) {
    	   System.out.println("hello == 3!");
       }
       
       if (myFirstNumber == 4) {
    	   System.out.println("myFirstNumber == 4!");
       }
    }
}
