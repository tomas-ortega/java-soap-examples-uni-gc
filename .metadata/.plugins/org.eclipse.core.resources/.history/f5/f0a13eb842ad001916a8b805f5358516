package com.uni.gc;


public class StudentDTO {
	private Integer id;
	private String name;
	
	public StudentDTO() {
		this.id = null;
		this.name = null;
	}
	
	public void setId(Integer newId) {
		this.id = newId;
	}
	
	public Integer getId() {
		return this.id;
	}
	
	public void setName(String newName) {
		this.name = newName;
	}
	
	public String getName() {
		return this.name;
	}
}