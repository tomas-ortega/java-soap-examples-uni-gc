<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           
			targetNamespace="tns:studentDTO">

<xs:simpleType name="studentNameValidator">
	<xs:restriction base="xs:string">
		<xs:pattern value="Pakito|Juanito"/>
	</xs:restriction>
</xs:simpleType>

<xs:element name="StudentDTO">
	<xs:complexType>
		<xs:sequence>
			<xs:element name="id" type="xs:int" minOccurs='1' maxOccurs='1' />
			<xs:element name="name" type="studentNameValidator" minOccurs='1' maxOccurs='1' />
		</xs:sequence>
	</xs:complexType>
</xs:element>
</xs:schema>